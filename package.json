{
  "name": "axentax-compiler-server",
  "version": "1.0.0",
  "description": "Axentax syntax compiler API server",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "bundle": "esbuild src/index.ts --platform=node --target=node18 --bundle --format=cjs --outfile=dist/index.js",
    "start": "npm run bundle && node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:watch": "ts-node --watch src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "axentax",
    "compiler",
    "api",
    "guitar",
    "midi"
  ],
  "author": "",
  "license": "LICENSE",
  "dependencies": {
    "axentax-compiler": "^1.0.1",
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "esbuild": "^0.25.9",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
